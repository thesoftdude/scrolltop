{
    "name": "softdude/scrolltop",
    "description": "A Craft CMS 5 plugin that adds a scroll-to-top button to your site.",
    "type": "craft-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "softdude",
            "homepage": "https://github.com/thesoftdude"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.1.2",
    "require": {
        "craftcms/cms": "^4.0.0|^5.0.0",
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "softdude\\scrolltop\\": "src/"
        }
    },
    "extra": {
        "name": "Scrolltop",
        "handle": "scrolltop",
        "class": "softdude\\scrolltop\\Scrolltop"
    }
} 